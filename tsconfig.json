{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "target": "ES2017",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "ES2021",
      "DOM"
    ],
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}